# ElevenLabs
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id
# Provide ONE of the following depending on your ElevenLabs/Twilio setup
# Preferred: agent phone number id managed by ElevenLabs
ELEVENLABS_AGENT_PHONE_NUMBER_ID=your_elevenlabs_phone_number_id
# Fallback: from_number if your setup expects a raw E.164 number
ELEVENLABS_FROM_NUMBER=+15551230000

# Webhook security (optional). If set, requests must include matching HMAC SHA256 in header 'ElevenLabs-Signature'
ELEVENLABS_WEBHOOK_SECRET=

# Server
PORT=5000
